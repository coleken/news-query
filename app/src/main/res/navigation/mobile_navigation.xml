<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/mobile_navigation"
  app:startDestination="@+id/navigation_world">

  <fragment
    android:id="@+id/navigation_world"
    android:label="@string/title_world"
    android:name="com.example.newsq.ui.world.WorldFragment"
    tools:layout="@layout/fragment_world" />

  <fragment
    android:id="@+id/navigation_browse"
    android:label="@string/title_browse"
    android:name="com.example.newsq.ui.browse.BrowseFragment"
    tools:layout="@layout/fragment_browse" />

  <fragment
    android:id="@+id/navigation_search"
    android:label="@string/title_search"
    android:name="com.example.newsq.ui.search.SearchFragment"
    tools:layout="@layout/fragment_search" />
</navigation>